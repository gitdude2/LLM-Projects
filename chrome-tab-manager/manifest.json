{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0",
    "description": "Extension for tab management including closing and putting tabs to sleep",
    "permissions": ["tabs", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Tab Manager"
    }
}